"*":
  "atom-material-ui":
    colors:
      abaseColor:
        red: 135
        green: 171
        blue: 4
        alpha: 1
      accentColor:
        red: 231
        green: 231
        blue: 231
        alpha: 1
      genAccent: true
    fonts: {}
    tabs: {}
    treeView:
      compactList: true
    ui:
      panelContrast: true
      panelShadows: true
  "autocomplete-plus":
    confirmCompletion: "tab"
  core:
    autoHideMenuBar: true
    destroyEmptyPanes: false
    disabledPackages: [
      "wrap-guide"
      "spell-check"
      "atomatigit"
      "vim-mode-clipboard-plus"
      "highlight-selected"
    ]
    themes: [
      "atom-material-ui"
      "itg-dark-syntax"
    ]
  editor:
    backUpBeforeSaving: true
    fontFamily: "input mono"
    fontSize: 13
    invisibles:
      tab: "â–»"
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 2
  "exception-reporting":
    userId: "c39a5722-f26c-ca60-77f1-a73b1e598e40"
  "git-plus":
    openInPane: false
  "highlight-line":
    enableSelectionBorder: true
    enableUnderline: true
  "highlight-selected":
    onlyHighlightWholeWords: false
  "keyboard-localization":
    useKeyboardLayoutFromPath: "/home/linucc/.atom/gb_locale.json"
  "multi-wrap-guide":
    columns: [
      80
      100
    ]
    locked: true
    silent: true
  pigments:
    markerType: "underline"
    mergeColorDuplicates: true
  "project-manager": {}
  "ruby-test":
    shell: "/bin/zsh"
    testFileCommand: "~/bin/blautotest {relative_path}"
  "save-session":
    dataSaveFolder: "/home/linucc/.atom/packages/save-session/projects"
  "vim-mode":
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
