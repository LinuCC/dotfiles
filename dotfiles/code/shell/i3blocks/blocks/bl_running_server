#/bin/bash
BLPATH="/home/linucc/code/brandslisten"
servers=($(ls "$BLPATH"))

server_running=false

for sv in ${servers[*]}
do
  if [ -r "$BLPATH/${sv}/tmp/pids/server.pid" ]
  then
    server_running=$(echo "$sv" | sed 's/bl_//')
  fi
done
if [ $server_running != false ]
then
  echo $server_running
  exit 0
else
  echo "ÔÅû"
  exit 0
fi
